<div class="container">
  <div class="row">
    <div class="p-2 bg-light text-black">ジャンル編集</div>
  </div>

  <br>

  <div class="row">
 　  <%= form_with model: @genre, url:admin_genre_path do |f| %>
    　<div class="form-group row">
    　　  <div><%= f.label :ジャンル名 %></div>
