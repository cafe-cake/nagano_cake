<h2>会員一覧</h2>
<table>
  <tbody>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  </tr>

  <% @members.each do |member| %>
  <tr>
    <td><%= @member.id %></td>
    <td><%= @member.last_name%><%= @member.first_name %></td>
    <% if is_deleted == false %>
    <td>退会</td>
    <% else %>
    <td>有効</td>
    <% end %>
  <% end %>
  </tr>
  </tbody>
</table>