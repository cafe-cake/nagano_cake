<main>
<h2>会員一覧</h2>
<table>
  <tbody>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  </tr>

  <% @members.each do |member| %>
  <tr>
    <td><%= member.id %></td>
    <td>
      <%= link_to admin_member_path(member) do %>
      <%= member.last_name%><%= member.first_name %>
      <% end %>
      </td>
    <td><%= member.email %></td>
    <td><%= member.is_deleted %></td>
  </tr>
  <% end %>
  </tbody>
</table>
<%= paginate @members %>
</main>